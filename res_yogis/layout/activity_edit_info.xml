    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="*"
        android:textColor="#FF0101"
        app:layout_constraintStart_toEndOf="@+id/textView23"
        tools:layout_editor_absoluteY="473dp" />

